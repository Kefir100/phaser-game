{
  "name": "PhaserTypeScriptWebpack",
  "version": "1.0.0",
  "description": "",
  "author": "Wicked City",
  "main": "index.js",
  "scripts": {
    "static": "webpack --watch --colors",
    "server": "node server/index.js",
    "server:watch": "nodemon server/index.js",
    "tunnel:dev": "ngrok http 4004",
    "tunnel:prod": "npm run build:prod && ngrok http 4004",
    "build:dev": "webpack --config webpack.config.js",
    "build:prod": "webpack --config webpack.production.config.js",
    "test": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix -c tslint.json",
      "git add"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "expose-loader": "0.7.3",
    "husky": "0.14.3",
    "lint-staged": "4.0.3",
    "ngrok": "2.2.22",
    "ts-loader": "2.2.2",
    "tslint": "5.6.0",
    "typescript": "2.4.1",
    "webpack": "2.6.1"
  },
  "dependencies": {
    "express": "4.16.1",
    "phaser-ce": "2.8.0",
    "socket.io": "2.0.3",
    "webfontloader": "1.6.28"
  }
}
